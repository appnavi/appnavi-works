{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "run-s build run",
    "run": "node ./dist/index.js",
    "dev": "run-p dev:*",
    "dev:server": "nodemon -L ./src/index.ts",
    "dev:sass": "sass --style=compressed --update --watch --poll public/stylesheets:public/stylesheets",
    "build": "run-s build:*",
    "build:server": "tsc -p tsconfig.json",
    "build:sass": "sass public/stylesheets:public/stylesheets --style=compressed --update",
    "check-types": "run-p check-types:*",
    "check-types:server": "tsc -p tsconfig.json --noEmit",
    "eslint:server": "eslint src/**/*.ts",
    "eslint:server:fix": "eslint src/**/*.ts --fix",
    "lint": "run-p eslint:server check-types",
    "format": "prettier --write src/**/*.{js,ts,json}",
    "lint:fix": "run-s check-types format eslint:server:fix ",
    "test": "nodemon -L --config nodemon.test.json"
  },
  "dependencies": {
    "@trpc/server": "^10.14.0",
    "compression": "^1.7.4",
    "connect-mongo": "^4.5.0",
    "cookie-parser": "^1.4.5",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-rate-limit": "^6.3.0",
    "express-session": "^1.17.2",
    "fs-extra": "^11.1.0",
    "helmet": "^6.0.0",
    "log4js": "^6.3.0",
    "mongodb": "^4.9.1",
    "mongoose": "^6.0.8",
    "multer": "^1.4.5-lts.1",
    "openid-client": "^5.1.8",
    "passport": "^0.6.0",
    "passport-custom": "^1.1.1",
    "passport-local": "^1.0.0",
    "sass": "^1.39.2",
    "serve-index": "^1.9.1",
    "superjson": "^1.12.2",
    "zod": "^3.20.2",
    "zod-validation-error": "^0.3.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-session": "^2.0.42",
    "@types/ejs": "^3.0.6",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.0.3",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/migrate-mongo": "^8.1.1",
    "@types/multer": "^1.4.6",
    "@types/node": "^18.0.3",
    "@types/passport": "^1.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/serve-index": "^1.9.0",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "cors": "^2.8.5",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^29.0.3",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.0",
    "supertest": "^6.1.3",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.1.0",
    "typescript": "^4.4.3"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{ts,json}": "prettier --write"
  }
}
