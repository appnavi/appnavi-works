{
  "name": "appnavi-works",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "run-s build run",
    "run": "node ./dist/index.js",
    "dev": "run-p dev:*",
    "dev:server": "nodemon -L ./src/index.ts",
    "dev:browser": "tsc -p tsconfig.browser.json -w",
    "dev:sass": "sass --style=compressed --update --watch --poll public/stylesheets:public/stylesheets private/stylesheets:private/stylesheets",
    "build": "run-s build:*",
    "build:server": "tsc -p tsconfig.json",
    "build:browser": "tsc -p tsconfig.browser.json",
    "build:sass": "sass public/stylesheets:public/stylesheets private/stylesheets:private/stylesheets --style=compressed --update",
    "check-types": "run-p check-types:*",
    "check-types:server": "tsc -p tsconfig.json --noEmit",
    "check-types:browser": "tsc -p tsconfig.browser.json --noEmit",
    "eslint:server": "eslint src/**/*.ts",
    "eslint:server:fix": "eslint src/**/*.ts --fix",
    "lint": "run-p eslint:server check-types",
    "format": "prettier --write src/**/*.{js,ts,json}",
    "lint:fix": "run-s check-types format eslint:server:fix ",
    "test": "nodemon -L --config nodemon.test.json",
    "prepare": "husky install",
    "migrate:create": "ts-node node_modules/.bin/migrate-mongo create -f migrate-mongo/migrate-mongo-config.ts --",
    "migrate:status": "ts-node node_modules/.bin/migrate-mongo status -f migrate-mongo/migrate-mongo-config.ts",
    "migrate:down": "ts-node node_modules/.bin/migrate-mongo down -f migrate-mongo/migrate-mongo-config.ts",
    "migrate:undo-last": "run-s migrate:down migrate:status",
    "migrate": "ts-node node_modules/.bin/migrate-mongo up -f migrate-mongo/migrate-mongo-config.ts"
  },
  "dependencies": {
    "@juris710/passport-slack": "^0.3.3",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "csurf": "^1.11.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "helmet": "^4.4.1",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.3.0",
    "mongoose": "^5.13.5",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "sass": "^1.37.5",
    "serve-index": "^1.9.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-session": "^2.0.42",
    "@types/csurf": "^1.11.1",
    "@types/ejs": "^3.0.6",
    "@types/express": "^4.17.11",
    "@types/fs-extra": "^9.0.8",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^27.0.0",
    "@types/jquery": "^3.5.5",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/materialize-css": "^1.0.11",
    "@types/migrate-mongo": "^8.1.1",
    "@types/mongoose": "^5.10.3",
    "@types/multer": "^1.4.6",
    "@types/node": "^16.4.13",
    "@types/passport": "^1.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/passport-oauth2": "^1.4.11",
    "@types/sass": "^1.16.0",
    "@types/serve-index": "^1.9.0",
    "@types/supertest": "^2.0.10",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.2",
    "migrate-mongo": "^8.2.2",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "passport-stub": "^1.1.1",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.4"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{ts,json}": "prettier --write"
  }
}
