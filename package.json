    "express-session": "^1.17.1",
    "fs-extra": "^9.0.1",
    "helmet": "^4.3.1",
    "http-errors": "~1.8.0",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.3.0",
    "morgan": "~1.10.0",
    "multer": "^1.4.2",
    "node-sass-middleware": "0.11.0",
    "request": "^2.88.2",
    "serve-index": "^1.9.1"
