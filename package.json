{
  "name": "game-upload",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "run-s build run",
    "run": "node ./dist/index.js",
    "dev": "run-p dev:*",
    "dev:server": "nodemon -L ./src/index.ts",
    "dev:browser": "tsc -p tsconfig.browser.json -w",
    "build": "run-s build:*",
    "build:server": "tsc -p tsconfig.json",
    "build:browser": "tsc -p tsconfig.browser.json",
    "check-types": "run-s check-types:*",
    "check-types:server": "tsc -p tsconfig.json --noEmit",
    "check-types:browser": "tsc -p tsconfig.browser.json --noEmit",
    "eslint:server": "eslint src/**/*.ts",
    "eslint:server:fix": "eslint src/**/*.ts --fix",
    "lint": "run-s eslint:server check-types",
    "format": "prettier --write src/**/*.{js,ts,json}",
    "lint:fix": "run-s check-types format eslint:server:fix ",
    "test": "nodemon -L --config nodemon.test.json"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "fs-extra": "^9.1.0",
    "helmet": "^4.4.1",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.3.0",
    "mongoose": "^5.11.18",
    "multer": "^1.4.2",
    "node-sass-middleware": "^0.11.0",
    "passport": "^0.4.1",
    "passport-slack": "juris710/passport-slack#v0.2.0",
    "request": "^2.88.2",
    "serve-index": "^1.9.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-session": "^2.0.42",
    "@types/ejs": "^3.0.6",
    "@types/express": "^4.17.11",
    "@types/fs-extra": "^9.0.8",
    "@types/http-errors": "^1.8.0",
    "@types/jquery": "^3.5.5",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/materialize-css": "^1.0.8",
    "@types/mocha": "^8.2.1",
    "@types/mongoose": "^5.10.3",
    "@types/multer": "^1.4.5",
    "@types/node-sass-middleware": "^0.0.31",
    "@types/passport": "^1.0.6",
    "@types/request": "^2.48.5",
    "@types/serve-index": "^1.7.30",
    "@types/supertest": "^2.0.10",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "chai": "^4.3.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.3.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "passport-stub": "^1.1.1",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2"
  }
}
