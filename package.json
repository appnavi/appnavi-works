{
  "name": "game-upload-dev",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "run-s build run",
    "run": "node ./dist/bin/www.js",
    "dev": "run-p dev:*",
    "dev:server": "nodemon -L ./src/bin/www",
    "dev:browser": "tsc -p tsconfig.browser.json -w",
    "build": "run-s build:*",
    "build:server": "tsc -p tsconfig.json",
    "build:browser": "tsc -p tsconfig.browser.json",
    "check-types": "run-s check-types:*",
    "check-types:server": "tsc -p tsconfig.json --noEmit",
    "check-types:browser": "tsc -p tsconfig.browser.json --noEmit",
    "eslint:server": "eslint src/**/*.ts",
    "eslint:server:fix": "eslint src/**/*.ts --fix",
    "lint": "run-s eslint:server check-types",
    "format": "prettier --write src/**/*.{js,ts,json}",
    "lint:fix": "run-s check-types format eslint:server:fix "
  },
  "dependencies": {
    "async-passport-slack": "^0.1.0",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "cookie-session": "^1.4.0",
    "debug": "~4.3.1",
    "diskusage": "^1.1.3",
    "dotenv": "^8.2.0",
    "ejs": "~3.1.5",
    "express": "~4.17.1",
    "fs-extra": "^9.0.1",
    "helmet": "^4.3.1",
    "http-errors": "~1.8.0",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.3.0",
    "mongoose": "^5.11.14",
    "morgan": "~1.10.0",
    "multer": "^1.4.2",
    "node-sass-middleware": "0.11.0",
    "passport": "^0.4.1",
    "request": "^2.88.2",
    "serve-index": "^1.9.1",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-session": "^2.0.42",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^8.2.0",
    "@types/ejs": "^3.0.5",
    "@types/escape-html": "^1.0.0",
    "@types/express": "^4.17.9",
    "@types/fs-extra": "^9.0.6",
    "@types/helmet": "^4.0.0",
    "@types/http-errors": "^1.8.0",
    "@types/jquery": "^3.5.5",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/log4js": "^2.3.5",
    "@types/materialize-css": "^1.0.8",
    "@types/mongoose": "^5.10.3",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/node-sass-middleware": "^0.0.31",
    "@types/passport": "^1.0.5",
    "@types/request": "^2.48.5",
    "@types/serve-index": "^1.7.30",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
